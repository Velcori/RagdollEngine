mouse.KeyDown:connect(function(key)
if key == "q" then --set key here
-- Script generated by SimpleSpy - credits to exx#9394
_G.Loop = false
while _G.Loop do wait()
game:GetService("Players").LocalPlayer.Character.Push.PushEvent:FireServer()
end
end
end)
