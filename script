local Plr = game:GetService("Players").LocalPlayer
local Mouse = Plr:GetMouse()

Mouse.Button1Down:connect(function()
if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
if not Mouse.Target then return end
Plr.Character:MoveTo(Mouse.Hit.p)
end)

local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()

mouse.KeyDown:connect(function(key)
if key == "g" then --set key here
-- Script generated by SimpleSpy - credits to exx#9394
_G.ToggleValue = true

local M = getrawmetatable(game)
local NC = M.__namecall
local RemoteLol = {"RagdollState", "LocalRagdollEvent"}
setreadonly(M,false)

M.__namecall = newcclosure(function(Remote, ...)
local arguments = {...}
if Remote.Name == RemoteLol[1] or Remote.Name == RemoteLol[2] and _G.ToggleValue == true then
return nil
end
return NC(Remote, ...)
end)
end
end)

local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()

mouse.KeyDown:connect(function(key)
if key == "h" then --set key here
_G.ToggleValue = false

local M = getrawmetatable(game)
local NC = M.__namecall
local RemoteLol = {"RagdollState", "LocalRagdollEvent"}
setreadonly(M,false)

M.__namecall = newcclosure(function(Remote, ...)
local arguments = {...}
if Remote.Name == RemoteLol[1] or Remote.Name == RemoteLol[2] and _G.ToggleValue == true then
return nil
end
return NC(Remote, ...)
end)
end
end)
local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()

mouse.KeyDown:connect(function(key)
if key == "j" then --set key here
local tool

for i,v in pairs (game.Players:GetPlayers()) do

 tool = v.Backpack:FindFirstChild'OddPotion' or v.Character:FindFirstChild'OddPotion'

 if tool then break end

end

 

for i,v in pairs(workspace.NewerMap:GetDescendants()) do

 tool.TransEvent:FireServer(v,1)

end
end
end)
local plr = game.Players.LocalPlayer
local mouse = plr:GetMouse()

mouse.KeyDown:connect(function(key)
if key == "k" then --set key here
local tool

for i,v in pairs (game.Players:GetPlayers()) do

 tool = v.Backpack:FindFirstChild'OddPotion' or v.Character:FindFirstChild'OddPotion'

 if tool then break end

end

 

for i,v in pairs(workspace.NewerMap:GetDescendants()) do

 tool.TransEvent:FireServer(v,0)

 if v:IsA("Part") and v.Name == ("Base") or v.Name == ("Hitbox") or v.Name == ("Hitbox") then
     tool.TransEvent:FireServer(v,1)
end
end
end

